import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int r=0;
           while (a > 0) {
            int digit = a%10;        
            r = r * 10 + digit;
            a = a / 10;               
        }
        System.out.println(r);
    }
}
