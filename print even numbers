import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner (System.in);
        int n=s.nextInt();
        for(int i=2;i<=n;i++)
        {
            if(i%2==0){
            System.out.print(i+" ");
            }
        }
    }
}
